{
  "name": "firebase-explorer-plus",
  "preview": true,
  "displayName": "Firebase Explorer Plus",
  "description": "Visual Studio Code extension from the forked project Firebase Explorer to explore and manage your Firebase projects",
  "version": "1.0.2",
  "author": {
    "name": "Jouca",
    "email": "joucagame@gmail.com"
  },
  "contributors": [
    {
      "email": "vymarkov@gmail.com",
      "name": "Vitaly Markov"
    },
    {
      "name": "Josep Sayol",
      "email": "josep.sayol@gmail.com"
    }
  ],
  "publisher": "Jouca",
  "homepage": "https://github.com/Jouca/vscode-firebase-explorer-plus/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/Jouca/vscode-firebase-explorer-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jouca/vscode-firebase-explorer-plus.git"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension",
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#039BE5",
    "theme": "dark"
  },
  "scripts": {
    "dev": "webpack --watch --env.development --info-verbosity verbose",
    "test": "yarn compile && node ./node_modules/vscode/bin/test",
    "build": "webpack --mode=development --env.development",
    "compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "webpack --mode=production --env.production",
    "package": "vsce package"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "firebase-explorer-plus",
          "title": "Firebase Explorer Plus",
          "icon": "assets/firebase.svg"
        }
      ]
    },
    "views": {
      "firebase-explorer-plus": [
        {
          "id": "firebase-projects",
          "name": "Accounts & Projects"
        },
        {
          "id": "firebase-firestore",
          "name": "Firestore"
        },
        {
          "id": "firebase-database",
          "name": "Database"
        },
        {
          "id": "firebase-functions",
          "name": "Cloud Functions"
        },
        {
          "id": "firebase-hosting",
          "name": "Hosting"
        },
        {
          "id": "firebase-apps",
          "name": "Mobile Apps"
        }
      ]
    },
    "commands": [
      {
        "command": "firebaseExplorerPlus.functions.refresh",
        "title": "Refresh Functions",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.functions.triggerHTTPS",
        "title": "Trigger HTTPS Function",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.functions.copyTrigger",
        "title": "Copy Trigger URL",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.functions.openInConsole.firebase",
        "title": "Open in Firebase Console",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.functions.openInConsole.cloud",
        "title": "Open in Cloud Console",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.functions.viewLogs",
        "title": "View Logs",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.functions.viewSource",
        "title": "View Source Code",
        "category": "Functions"
      },
      {
        "command": "firebaseExplorerPlus.accounts.add",
        "title": "Add Account",
        "category": "Firebase",
        "icon": {
          "light": "assets/light/account-add.svg",
          "dark": "assets/dark/account-add.svg"
        }
      },
      {
        "command": "firebaseExplorerPlus.accounts.remove",
        "title": "Remove this account",
        "category": "Firebase"
      },
      {
        "command": "firebaseExplorerPlus.accounts.clearAll",
        "title": "Clear All Accounts",
        "category": "Firebase"
      },
      {
        "command": "firebaseExplorerPlus.openSettings",
        "title": "Open Extension Settings",
        "category": "Firebase",
        "icon": "$(gear)"
      },
      {
        "command": "firebaseExplorerPlus.projects.refresh",
        "title": "Refresh Projects",
        "category": "Firebase",
        "icon": {
          "light": "assets/light/refresh.svg",
          "dark": "assets/dark/refresh.svg"
        }
      },
      {
        "command": "firebaseExplorerPlus.projects.selection",
        "title": "Select Project",
        "category": "Firebase"
      },
      {
        "command": "firebaseExplorerPlus.apps.create",
        "title": "Add New App",
        "category": "Firebase",
        "icon": {
          "light": "assets/light/add.svg",
          "dark": "assets/dark/add.svg"
        }
      },
      {
        "command": "firebaseExplorerPlus.apps.refresh",
        "title": "Refresh Apps",
        "category": "Firebase",
        "icon": {
          "light": "assets/light/refresh.svg",
          "dark": "assets/dark/refresh.svg"
        }
      },
      {
        "command": "firebaseExplorerPlus.apps.editAppName",
        "title": "Edit Name",
        "category": "Firebase Apps"
      },
      {
        "command": "firebaseExplorerPlus.apps.showAppConfig",
        "title": "Show Configuration",
        "category": "Firebase Apps"
      },
      {
        "command": "firebaseExplorerPlus.apps.showAppCertificates",
        "title": "Show SHA Certificates",
        "category": "Firebase Apps"
      },
      {
        "command": "firebaseExplorerPlus.apps.addAppCertificate",
        "title": "Add new fingerprint",
        "category": "Firebase Apps"
      },
      {
        "command": "firebaseExplorerPlus.apps.copyAppCertificate",
        "title": "Copy fingerprint",
        "category": "Firebase Apps"
      },
      {
        "command": "firebaseExplorerPlus.apps.deleteAppCertificate",
        "title": "Delete fingerprint",
        "category": "Firebase Apps"
      },
      {
        "command": "firebaseExplorerPlus.firestore.refresh",
        "title": "Refresh Data",
        "category": "Firestore",
        "icon": {
          "light": "assets/light/refresh.svg",
          "dark": "assets/dark/refresh.svg"
        }
      },
      {
        "command": "firebaseExplorerPlus.firestore.addCollection",
        "title": "Add Collection",
        "category": "Firestore",
        "icon": {
          "light": "assets/light/add.svg",
          "dark": "assets/dark/add.svg"
        }
      },
      {
        "command": "firebaseExplorerPlus.firestore.refreshCollection",
        "title": "Refresh Collection",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copyItemName",
        "title": "Copy Name",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copyItemPath",
        "title": "Copy Path",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copySnippet.JS.ref",
        "title": "Copy Reference Snippet (JS)",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copySnippet.JS.doc.onSnapshot",
        "title": "Copy onSnapshot Snippet (JS)",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copySnippet.JS.collection.onSnapshot",
        "title": "Copy onSnapshot Snippet (JS)",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.refreshDocument",
        "title": "Refresh Document",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.addDocument",
        "title": "Add Document",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.deleteCollection",
        "title": "Delete Collection",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.exportDatabase",
        "title": "Export Database to JSON",
        "category": "Firestore",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "firebaseExplorerPlus.firestore.importDatabase",
        "title": "Import Database from JSON",
        "category": "Firestore",
        "icon": "$(cloud-download)"
      },
      {
        "command": "firebaseExplorerPlus.firestore.clearDatabase",
        "title": "Clear Entire Database",
        "category": "Firestore",
        "icon": "$(trash)"
      },
      {
        "command": "firebaseExplorerPlus.firestore.deleteDocument",
        "title": "Delete Document",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copyDocumentContent",
        "title": "Copy Content",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.viewDocumentContent",
        "title": "View as JSON",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copyDocumentFieldName",
        "title": "Copy Field Name",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.firestore.copyDocumentFieldValue",
        "title": "Copy Field Value",
        "category": "Firestore"
      },
      {
        "command": "firebaseExplorerPlus.database.refresh",
        "title": "Refresh Database",
        "category": "Database"
      },
      {
        "command": "firebaseExplorerPlus.database.copyName",
        "title": "Copy Name",
        "category": "Database"
      },
      {
        "command": "firebaseExplorerPlus.database.copyPath",
        "title": "Copy Path",
        "category": "Database"
      },
      {
        "command": "firebaseExplorerPlus.database.copySnippet.JS.ref",
        "title": "Copy Reference Snippet (JS)",
        "category": "Database"
      },
      {
        "command": "firebaseExplorerPlus.database.copySnippet.JS.onValue",
        "title": "Copy On-Value Snippet (JS)",
        "category": "Database"
      },
      {
        "command": "firebaseExplorerPlus.database.deleteEntry",
        "title": "Remove Entry (And Children)",
        "category": "Database"
      },
      {
        "command": "firebaseExplorerPlus.database.editEntryValue",
        "title": "Edit Value",
        "category": "Database"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "firebaseExplorerPlus.accounts.add",
          "when": "view == firebase-projects",
          "group": "navigation@1"
        },
        {
          "command": "firebaseExplorerPlus.projects.refresh",
          "when": "view == firebase-projects",
          "group": "navigation@2"
        },
        {
          "command": "firebaseExplorerPlus.openSettings",
          "when": "view =~ /^firebase-/",
          "group": "navigation@99"
        },
        {
          "command": "firebaseExplorerPlus.apps.create",
          "when": "view == firebase-apps && firebase:apps:loaded",
          "group": "navigation@1"
        },
        {
          "command": "firebaseExplorerPlus.apps.refresh",
          "when": "view == firebase-apps && firebase:apps:loaded",
          "group": "navigation@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.addCollection",
          "when": "view == firebase-firestore && firebase:firestore:loaded",
          "group": "navigation@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.refresh",
          "when": "view == firebase-firestore && firebase:firestore:loaded",
          "group": "navigation@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.exportDatabase",
          "when": "view == firebase-firestore && firebase:firestore:loaded",
          "group": "navigation@3"
        },
        {
          "command": "firebaseExplorerPlus.firestore.importDatabase",
          "when": "view == firebase-firestore && firebase:firestore:loaded",
          "group": "navigation@4"
        },
        {
          "command": "firebaseExplorerPlus.firestore.clearDatabase",
          "when": "view == firebase-firestore && firebase:firestore:loaded",
          "group": "navigation@5"
        }
      ],
      "view/item/context": [
        {
          "command": "firebaseExplorerPlus.projects.refresh",
          "when": "viewItem == account",
          "group": "1_account@1"
        },
        {
          "command": "firebaseExplorerPlus.accounts.remove",
          "when": "viewItem == account",
          "group": "1_account@2"
        },
        {
          "command": "firebaseExplorerPlus.functions.copyTrigger",
          "when": "viewItem == functions.ofTriggerType.https",
          "group": "functions.copy@1"
        },
        {
          "command": "firebaseExplorerPlus.functions.triggerHTTPS",
          "when": "viewItem == functions.ofTriggerType.https",
          "group": "functions.trigger@1"
        },
        {
          "command": "firebaseExplorerPlus.functions.openInConsole.firebase",
          "when": "viewItem =~ /^functions.ofTriggerType/",
          "group": "functions.openInConsole@1"
        },
        {
          "command": "firebaseExplorerPlus.functions.openInConsole.cloud",
          "when": "viewItem =~ /^functions.ofTriggerType/",
          "group": "functions.openInConsole@2"
        },
        {
          "command": "firebaseExplorerPlus.functions.viewLogs",
          "when": "viewItem =~ /^functions.ofTriggerType/",
          "group": "functions.details@1"
        },
        {
          "command": "firebaseExplorerPlus.functions.viewSource",
          "when": "viewItem =~ /^functions.ofTriggerType/",
          "group": "functions.details@2"
        },
        {
          "command": "firebaseExplorerPlus.database.copyName",
          "when": "viewItem =~ /^database\\.(entry|parentEntry|valueEntry)/",
          "group": "database.entry-copy@1"
        },
        {
          "command": "firebaseExplorerPlus.database.copyPath",
          "when": "viewItem =~ /^database\\.(entry|parentEntry|valueEntry)/",
          "group": "database.entry-copy@2"
        },
        {
          "command": "firebaseExplorerPlus.database.copySnippet.JS.ref",
          "when": "viewItem =~ /^database\\.(entry|parentEntry|valueEntry)/",
          "group": "database.entry-snippet@1"
        },
        {
          "command": "firebaseExplorerPlus.database.copySnippet.JS.onValue",
          "when": "viewItem =~ /^database\\.(entry|parentEntry|valueEntry)/",
          "group": "database.entry-snippet@2"
        },
        {
          "command": "firebaseExplorerPlus.database.editEntryValue",
          "when": "viewItem == database.valueEntry",
          "group": "database.entry-edit@1"
        },
        {
          "command": "firebaseExplorerPlus.database.deleteEntry",
          "when": "viewItem =~ /^database\\.(entry|parentEntry|valueEntry)/",
          "group": "database.entry-edit@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyItemName",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-copy@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyItemPath",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-copy@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copySnippet.JS.ref",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-snippet@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copySnippet.JS.collection.onSnapshot",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-snippet@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.addDocument",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-edit@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.deleteCollection",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-edit@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.refreshCollection",
          "when": "viewItem == firestore.collection",
          "group": "firestore.collection-other@3"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyItemName",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-copy@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyItemPath",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-copy@2"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyDocumentContent",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-copy@3"
        },
        {
          "command": "firebaseExplorerPlus.firestore.viewDocumentContent",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-view@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.deleteDocument",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-edit@4"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copySnippet.JS.ref",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-snippet@5"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copySnippet.JS.doc.onSnapshot",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-snippet@6"
        },
        {
          "command": "firebaseExplorerPlus.firestore.refreshDocument",
          "when": "viewItem == firestore.document",
          "group": "firestore.document-other@7"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyDocumentFieldName",
          "when": "viewItem == firestore.documentField",
          "group": "firestore.documentField@1"
        },
        {
          "command": "firebaseExplorerPlus.firestore.copyDocumentFieldValue",
          "when": "viewItem == firestore.documentField",
          "group": "firestore.documentField@2"
        },
        {
          "command": "firebaseExplorerPlus.apps.editAppName",
          "when": "viewItem == apps.iosApp",
          "group": "apps@1"
        },
        {
          "command": "firebaseExplorerPlus.apps.showAppConfig",
          "when": "viewItem == apps.iosApp",
          "group": "apps@2"
        },
        {
          "command": "firebaseExplorerPlus.apps.editAppName",
          "when": "viewItem == apps.androidApp",
          "group": "apps@1"
        },
        {
          "command": "firebaseExplorerPlus.apps.showAppConfig",
          "when": "viewItem == apps.androidApp",
          "group": "apps@2"
        },
        {
          "command": "firebaseExplorerPlus.apps.showAppCertificates",
          "when": "viewItem == apps.androidApp",
          "group": "apps@3"
        },
        {
          "command": "firebaseExplorerPlus.apps.addAppCertificate",
          "when": "viewItem == apps.androidApp.fingerprintsFolder",
          "group": "apps.fingerprintsFolder@1"
        },
        {
          "command": "firebaseExplorerPlus.apps.copyAppCertificate",
          "when": "viewItem == apps.androidApp.fingerprint",
          "group": "apps.fingerprint@1"
        },
        {
          "command": "firebaseExplorerPlus.apps.deleteAppCertificate",
          "when": "viewItem == apps.androidApp.fingerprint",
          "group": "apps.fingerprint@2"
        }
      ]
    },
    "configuration": {
      "title": "Firebase Explorer Plus",
      "properties": {
        "firebaseExplorerPlus.firestore.autoOpenDocument": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open documents in editor after creation"
        }
      }
    }
  },
  "devDependencies": {
    "@types/chardet": "^0.5.0",
    "@types/clipboardy": "^1.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/iconv-lite": "^0.0.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^16.18.126",
    "@types/request-promise-native": "^1.0.15",
    "@types/semver": "^5.5.0",
    "@types/tmp": "^0.0.33",
    "@types/universal-analytics": "^0.4.2",
    "@types/vscode": "^1.50.0",
    "@types/webpack": "^4.4.20",
    "@types/yauzl": "^2.9.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "filemanager-webpack-plugin": "^9.0.1",
    "ovsx": "^0.10.9",
    "prettier": "^1.15.3",
    "terser-webpack-plugin": "^5.3.16",
    "ts-loader": "^9.5.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.4.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "vsce": "^1.94.0",
    "vscode": "^0.9.9",
    "vscode-test": "^1.1.0",
    "webpack": "^5.105.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "bulma": "^0.7.4",
    "bulma-badge": "^3.0.0",
    "bulma-checkradio": "^2.1.0",
    "bulma-divider": "^0.2.0",
    "bulma-switch": "^2.0.0",
    "chardet": "^0.7.0",
    "directory-tree": "^2.2.7",
    "dotenv": "^8.2.0",
    "firebase-tools": "^15.7.0",
    "iconv-lite": "^0.4.24",
    "jsonwebtoken": "^9.0.3",
    "mime-types": "^2.1.21",
    "mkdirp": "^0.5.1",
    "portfinder": "^1.0.20",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.9",
    "semver": "^5.6.0",
    "tmp": "^0.2.5",
    "tslib": "^1.9.3",
    "yauzl": "^2.10.0"
  },
  "resolutions": {
    "request/tough-cookie": "2.5.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80
  }
}
